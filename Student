
public class Student {


		int id;
		int Grade;
		String name;
		String [] Subjects;
		
		public void setID(int x) {
		id = x;
		
		


	
		}
		
	//parameteised constructor

		public Student(int id, int grade, String name, String[] subjects) {
			super();
			this.id = id;
			Grade = grade;
			this.name = name;
			Subjects = subjects;
		}

		
		//this is defult constructor
		public Student() {
			super();
		}



		/**
		 * @return the id
		 */
		public int getId() {
			return id;
		}

		/**
		 * @param id the id to set
		 */
		public void setId(int id) {
			this.id = id;
		}

		/**
		 * @return the grade
		 */
		public int getGrade() {
			return Grade;
		}

		/**
		 * @param grade the grade to set
		 */
		public void setGrade(int grade) {
			Grade = grade;
		}

		/**
		 * @return the name
		 */
		public String getName() {
			return name;
		}

		/**
		 * @param name the name to set
		 */
		public void setName(String name) {
			this.name = name;
		}

		/**
		 * @return the subjects
		 */
		public String[] getSubjects() {
			return Subjects;
		}

		/**
		 * @param subjects the subjects to set
		 */
		public void setSubjects(String[] subjects) {
			Subjects = subjects;
		}

}


////////////////



public class Starter {

	public static void main(String[] args) {

		
		Student s;
		s = new Student();
				String [] subs = {"Math" , "Eco" , "Science"};
		
		s.setId(5);
		s.setName("Sara");
		s.setGrade(1);
		s.setSubjects(subs);
		
		
		System.out.println("The Id is "+s.getId());
		System.out.println("The Name is "+s.getName());
		System.out.println("The Grade is "+s.getGrade());

		for(int i =0;i<3;i++) {
				System.out.println("The " +i+" Subject is " +s.getSubjects()[i]);
				 
		}
		
		
		
		
	}

}
